@model IEnumerable<SportsFinder.Data.Models.WeatherForecast>
@{
    ViewData["Title"] = "RazorGet";
}

<h1>Form</h1>


<div>
    <table class="table">
        <thead>
            <tr>
                <th>Date</th>
                <th>Temp. (C)</th>
                <th>Temp. (F)</th>
                <th>Summary</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var forecast in Model)
            {
            <tr>
                <td>@forecast.DateFormatted</td>
                <td>@forecast.TemperatureC</td>
                <td>@forecast.TemperatureF</td>
                <td>@forecast.Summary</td>
            </tr>
            }
        </tbody>
    </table>
</div>



@section Scripts {


}